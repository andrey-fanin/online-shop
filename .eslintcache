number) => {\r\n        setInfo(info.filter(i => i.number !== number))\r\n    }\r\n    const changeInfo = (key, value, number) => {\r\n        setInfo(info.map(i => i.number === number ? {...i, [key]:value} : i))\r\n    }\r\n\r\n    const selectFile = e => {\r\n        setFile(e.target.files[0])\r\n    }\r\n\r\n    const addDevice = () => {\r\n        const formData = new FormData()\r\n        formData.append('name', name)\r\n        formData.append('price', `${price}`)\r\n        formData.append('img', file)\r\n        formData.append('brandId', device.selectedBrand.id)\r\n        formData.append('brandName', device.selectedBrand.name)\r\n        formData.append('typeId', device.selectedType.id)\r\n        formData.append('info', JSON.stringify(info))\r\n        createDevice(formData).then(data => onHide())\r\n    }\r\n\r\n    return (\r\n        <Modal\r\n            show={show}\r\n            onHide={onHide}\r\n            size=\"lg\"\r\n            centered\r\n        >\r\n            <Modal.Header closeButton>\r\n                <Modal.Title id=\"contained-modal-title-vcenter\">\r\n                    Add device\r\n                </Modal.Title>\r\n            </Modal.Header>\r\n            <Modal.Body>\r\n                <Form>\r\n                    <Dropdown className='mt-2, mb-2'>\r\n                        <Dropdown.Toggle>\r\n                            {device.selectedType.name || 'Select type'}\r\n                        </Dropdown.Toggle>\r\n                        <Dropdown.Menu>\r\n                            {device.types.map(type =>\r\n                                <Dropdown.Item onClick={() => device.setSelectedType(type)} key={type.id}>{type.name}</Dropdown.Item>\r\n                            )}\r\n                        </Dropdown.Menu>\r\n                    </Dropdown>\r\n                    <Dropdown className='mt-2, mb-2'>\r\n                        <Dropdown.Toggle>\r\n                            {device.selectedBrand.name || 'Select brand'}\r\n                        </Dropdown.Toggle>\r\n                        <Dropdown.Menu>\r\n                            {device.brands.map(brand =>\r\n                                <Dropdown.Item onClick={() => device.setSelectedBrand(brand)} key={brand.name}>{brand.name}</Dropdown.Item>\r\n                            )}\r\n                        </Dropdown.Menu>\r\n                    </Dropdown>\r\n                    <Form.Control\r\n                        value={name}\r\n                        onChange={e => setName(e.target.value)}\r\n                        className='mt-3'\r\n                        placeholder='Enter the name of device'\r\n                        type='text'/>\r\n                    <Form.Control\r\n                        value={price}\r\n                        onChange={e => setPrice(Number(e.target.value))}\r\n                        className='mt-3'\r\n                        placeholder='Enter the price of device'\r\n                        type='number'/>\r\n                    <Form.Control\r\n                        className='mt-3'\r\n                        placeholder='Enter the image of device'\r\n                        type='file'\r\n                        onChange={selectFile}\r\n                    />\r\n                    <hr/>\r\n                    <Button\r\n                        variant={'outline-dark'}\r\n                        onClick={addInfo}\r\n                    >Add new feature</Button>\r\n                    {\r\n                        info.map(i =>\r\n                            <Row className='mt-4' key={i.number}>\r\n                                <Col md={4}>\r\n                                    <Form.Control\r\n                                        value={i.title}\r\n                                        onChange={e => changeInfo('title', e.target.value, i.number)}\r\n                                        placeholder='Enter the name of feature'\r\n                                    />\r\n                                </Col>\r\n                                <Col md={4}>\r\n                                    <Form.Control\r\n                                        value={i.description}\r\n                                        onChange={e => changeInfo('description', e.target.value, i.number)}\r\n                                        placeholder='Enter the description of feature'\r\n                                    />\r\n                                </Col>\r\n                                <Col md={4}>\r\n                                    <Button\r\n                                        onClick={() => removeInfo(i.number)}\r\n                                        variant={'outline-danger'}>Delete</Button>\r\n                                </Col>\r\n                            </Row>\r\n                        )\r\n                    }\r\n                </Form>\r\n            </Modal.Body>\r\n            <Modal.Footer>\r\n                <Button variant={'outline-danger'} onClick={onHide}>Close</Button>\r\n                <Button variant={'outline-success'} onClick={addDevice}>Add</Button>\r\n            </Modal.Footer>\r\n        </Modal>\r\n    )\r\n})\r\n\r\nexport default CreateDevice;","C:\\Users\\Andrey\\Desktop\\react-practice\\ulbi-shop\\client\\src\\components\\BrandBar.js",[],"C:\\Users\\Andrey\\Desktop\\react-practice\\ulbi-shop\\client\\src\\components\\TypeBar.js",[],"C:\\Users\\Andrey\\Desktop\\react-practice\\ulbi-shop\\client\\src\\components\\DeviceItem.js",["252","253","254"],"import React, {useContext} from 'react';\r\nimport {Card, Col, Image} from 'react-bootstrap'\r\nimport SvgStar from \"../assets/SvgStar\";\r\nimport {useHistory, useLocation} from \"react-router-dom\";\r\nimport {BASKET_ROUTE, DEVICE_ROUTE} from \"../utils/consts\";\r\nimport {Context} from \"../index\";\r\n\r\nconst DeviceItem = ({item}) => {\r\n    const history = useHistory()\r\n    const {device} = useContext(Context)\r\n\r\n